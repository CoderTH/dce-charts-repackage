{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "prometheus-adapter": {
            "type": "object",
            "properties": {
                "affinity": {
                    "type": "object"
                },
                "customAnnotations": {
                    "type": "object"
                },
                "customLabels": {
                    "type": "object"
                },
                "deploymentAnnotations": {
                    "type": "object"
                },
                "dnsConfig": {
                    "type": "object"
                },
                "env": {
                    "type": "array"
                },
                "extraArguments": {
                    "type": "array"
                },
                "extraVolumeMounts": {
                    "type": "array"
                },
                "extraVolumes": {
                    "type": "array"
                },
                "hostNetwork": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "pullPolicy": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "listenPort": {
                    "type": "integer"
                },
                "livenessProbe": {
                    "type": "object",
                    "properties": {
                        "httpGet": {
                            "type": "object",
                            "properties": {
                                "path": {
                                    "type": "string"
                                },
                                "port": {
                                    "type": "string"
                                },
                                "scheme": {
                                    "type": "string"
                                }
                            }
                        },
                        "initialDelaySeconds": {
                            "type": "integer"
                        },
                        "timeoutSeconds": {
                            "type": "integer"
                        }
                    }
                },
                "logLevel": {
                    "type": "integer"
                },
                "metricsRelistInterval": {
                    "type": "string"
                },
                "namespaceOverride": {
                    "type": "string"
                },
                "nodeSelector": {
                    "type": "object"
                },
                "podAnnotations": {
                    "type": "object"
                },
                "podLabels": {
                    "type": "object"
                },
                "priorityClassName": {
                    "type": "string"
                },
                "prometheus": {
                    "type": "object",
                    "properties": {
                        "path": {
                            "type": "string"
                        },
                        "port": {
                            "type": "integer"
                        },
                        "url": {
                            "type": "string"
                        }
                    }
                },
                "readinessProbe": {
                    "type": "object",
                    "properties": {
                        "httpGet": {
                            "type": "object",
                            "properties": {
                                "path": {
                                    "type": "string"
                                },
                                "port": {
                                    "type": "string"
                                },
                                "scheme": {
                                    "type": "string"
                                }
                            }
                        },
                        "initialDelaySeconds": {
                            "type": "integer"
                        },
                        "timeoutSeconds": {
                            "type": "integer"
                        }
                    }
                },
                "replicas": {
                    "type": "integer"
                },
                "resources": {
                    "type": "object"
                },
                "strategy": {
                    "type": "object",
                    "properties": {
                        "rollingUpdate": {
                            "type": "object",
                            "properties": {
                                "maxSurge": {
                                    "type": "string"
                                },
                                "maxUnavailable": {
                                    "type": "string"
                                }
                            }
                        },
                        "type": {
                            "type": "string"
                        }
                    }
                },
                "tolerations": {
                    "type": "array"
                },
                "topologySpreadConstraints": {
                    "type": "array"
                }
            }
        }
    }
}
